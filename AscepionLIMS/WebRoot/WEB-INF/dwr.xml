<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN"
    "http://getahead.org/dwr/dwr20.dtd">


<dwr>

	<init>
		<!--<converter id="testbean" class="org.directwebremoting.servlet.DwrServlet"/>-->
	</init>

	<allow>
		<create creator="new" javascript="ChemicalMolecularRepository">
			<param name="class" value="com.ascepionpharm.lims.repository.chemistry.ChemicalMolecularRepository" />
			<include method="checkInnerName" />
		</create>
		
		<create creator="new" javascript="ProjectRepository">
			<param name="class" value="com.ascepionpharm.lims.repository.core.ProjectRepository" />
			<include method="getByDepartment" />
		</create>
		
		<create creator="new" javascript="ReactionRepository">
			<param name="class" value="com.ascepionpharm.lims.repository.chemistry.ReactionRepository" />
			<include method="getNameBySmilesAndProject" />
		</create>
		
		<convert converter="bean" match="com.ascepionpharm.lims.entity.core.ProjectBean"></convert>
		<convert converter="bean" match="com.ascepionpharm.lims.entity.chemistry.ReactionBean"></convert>
	</allow>

	<signatures>

	</signatures>

</dwr>
